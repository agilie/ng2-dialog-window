{
  "name": "@agilie/ng2-dialog-window",
  "version": "0.2.0-alpha1",
  "description": "Modal dialogs and popups module for Angular applications",
  "scripts": {
    "build": "gulp build",
    "build:watch": "gulp",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p tsconfig.json -n ng2-dialog-window -d docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "lint": "tslint --type-check --project tsconfig.json src/**/*.ts",
    "test": "tsc && karma start",
    "prepub": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agilie/ng2-dialog-window"
  },
  "author": "Agilie Team <info@agilie.com> (https://agilie.com)",
  "contributors": [
    "Maks Nechesonov <maks.nechesonov@agilie.com> (https://github.com/NechiK)"
  ],
  "keywords": [
    "angular",
    "modals",
    "popups",
    "dialog",
    "ng2",
    "dynamic",
    "template"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/agilie/ng2-dialog-window/issues"
  },
  "peerDependencies": {
    "@angular/core": "^2.0.0 || ^4.0.0",
    "rxjs": "^5.1.0",
    "zone.js": "^0.7.8 || ^0.8.4"
  }
}
